    "eslint": "^8.57.0",
    "prettier": "^3.0.3",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^2.0.5",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "viem": "^2.21.54"
  },
  "peerDependencies": {
    "viem": "^2.21.54"
  }
